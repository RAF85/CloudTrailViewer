<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ListView?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="io.haskins.java.cloudtrailviewer.controller.dialog.preferences.ResourcesPanelController">

    <center>
        <ListView prefHeight="200.0" prefWidth="242.0" BorderPane.alignment="CENTER" fx:id="listView" editable="true" />
    </center>
    <bottom>
        <HBox prefHeight="35.0" prefWidth="242.0" BorderPane.alignment="CENTER">
            <Button mnemonicParsing="false" text="+" onAction="#add">
                <HBox.margin>
                    <Insets left="3.0" right="3.0"/>
                </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="-" onAction="#remove">
                <HBox.margin>
                    <Insets left="3.0" right="3.0"/>
                </HBox.margin>
            </Button>
            <BorderPane.margin>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0"/>
            </BorderPane.margin>
        </HBox>
    </bottom>
</BorderPane>