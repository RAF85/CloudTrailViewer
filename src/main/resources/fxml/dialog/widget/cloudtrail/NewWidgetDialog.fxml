<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<?import javafx.scene.layout.VBox?>
<BorderPane prefHeight="167.0"
            prefWidth="398.0"
            xmlns="http://javafx.com/javafx/8.0.111"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="io.haskins.java.cloudtrailviewer.controller.dialog.widget.NewWidgetDialogController">
    <bottom>
        <HBox alignment="CENTER_RIGHT" prefHeight="51.0" prefWidth="398.0" BorderPane.alignment="CENTER">
            <Button mnemonicParsing="false" text="Cancel" onAction="#handleCancel">
                <HBox.margin>
                    <Insets left="5.0" right="5.0"/>
                </HBox.margin>
            </Button>
        </HBox>
    </bottom>
    <center>
        <VBox>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                <Button fx:id="btnMap" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Map" onAction="#MapSelected">
                    <HBox.margin><Insets left="5.0" right="5.0" top="5.0"/></HBox.margin>
                </Button>
                <Button fx:id="btnTable" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Table" onAction="#TableSelected">
                    <HBox.margin><Insets left="5.0" right="5.0" top="5.0"/></HBox.margin>
                </Button>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                <Button fx:id="btnPieChart" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Pie Chart" onAction="#PieChartSelected">
                    <HBox.margin><Insets left="5.0" right="5.0" top="5.0"/></HBox.margin>
                </Button>
                <Button fx:id="btnBarChart" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Bar Chart" onAction="#BarChartSelected">
                    <HBox.margin><Insets left="5.0" right="5.0" top="5.0"/></HBox.margin>
                </Button>
                <Button fx:id="btnSBarChart" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Stacked Bar Chart" onAction="#StackedBarChartSelected" disable="true">
                    <HBox.margin><Insets left="5.0" right="5.0" top="5.0"/></HBox.margin>
                </Button>
                <Button fx:id="btnLineChart" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Line Chart" onAction="#LineChartSelected" disable="true">
                    <HBox.margin><Insets left="5.0" right="5.0" top="5.0"/></HBox.margin>
                </Button>
            </HBox>

            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                <Button fx:id="btnErros" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Errors" onAction="#ErrorsSelected">
                    <HBox.margin><Insets left="5.0" right="5.0" top="5.0"/></HBox.margin>
                </Button>
                <Button fx:id="btnResources" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Resources" onAction="#ResourcesSelected">
                    <HBox.margin><Insets left="5.0" right="5.0" top="5.0"/></HBox.margin>
                </Button>
                <Button fx:id="btnSecurity" mnemonicParsing="false" prefHeight="100.0" prefWidth="100.0" text="Security" onAction="#SecuritySelected">
                    <HBox.margin><Insets left="5.0" right="5.0" top="5.0"/></HBox.margin>
                </Button>
            </HBox>
        </VBox>
    </center>
</BorderPane>
